            android:name="com.example.clienteenergycalc.TelaInicial"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.clienteenergycalc.NovoAparelho"
            android:label="@string/title_activity_novo_aparelho" >
        </activity>
        <activity
            android:name="com.example.clienteenergycalc.Resultado"
            android:label="@string/title_activity_resultado" >
        </activity>
        <activity
            android:name="com.example.clienteenergycalc.Ranking"
            android:label="@string/title_activity_ranking" >
        </activity>
    </application>

</manifest>
